# Model selection (optional if set via `shandu configure`)
SHANDU_MODEL=deepseek/deepseek-chat

# Compatibility fallback model env var
OPENAI_MODEL_NAME=

# Provider key mapping used by Shandu runtime
# Example values: DEEPSEEK_API_KEY, OPENROUTER_API_KEY, ANTHROPIC_API_KEY, ANYSUPPORTED_API_KEY
SHANDU_API_KEY_ENV=DEEPSEEK_API_KEY

# Provider API key value used when SHANDU_API_KEY_ENV is set
SHANDU_API_KEY=your_provider_api_key_here

# Optional direct provider env vars (LiteLLM style). Use what matches your model provider.
DEEPSEEK_API_KEY=
OPENROUTER_API_KEY=
ANTHROPIC_API_KEY=
OPENAI_API_KEY=

# Generation controls
SHANDU_TEMPERATURE=0.2
SHANDU_MAX_TOKENS=8192

# Runtime storage
SHANDU_STORAGE_DIR=.blackgeorge

# Optional HTTP proxy for scraping
SHANDU_PROXY=
